{
  "name": "uga-campus-explorer",
  "version": "1.0.0",
  "description": "UGA Campus Explorer - A location-based campus discovery game",
  "private": true,
  "workspaces": [
    "frontend",
    "backend"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:backend": "cd backend && npm run dev",
    "build": "npm run build:frontend && npm run build:backend",
    "build:frontend": "cd frontend && npm run build",
    "build:backend": "cd backend && npm run build",
    "start": "npm run start:backend",
    "start:frontend": "cd frontend && npm run preview",
    "start:backend": "cd backend && npm start",
    "test": "npm run test:frontend && npm run test:backend",
    "test:frontend": "cd frontend && npm test",
    "test:backend": "cd backend && npm test",
    "lint": "npm run lint:frontend && npm run lint:backend",
    "lint:frontend": "cd frontend && npm run lint",
    "lint:backend": "cd backend && npm run lint",
    "seed": "cd backend && npm run seed",
    "setup": "npm install && npm run setup:frontend && npm run setup:backend",
    "setup:frontend": "cd frontend && npm install",
    "setup:backend": "cd backend && npm install",
    "clean": "rm -rf node_modules frontend/node_modules backend/node_modules",
    "reset": "npm run clean && npm run setup"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/uga-campus-explorer.git"
  },
  "keywords": [
    "uga",
    "campus",
    "explorer",
    "game",
    "react",
    "node",
    "supabase"
  ],
  "author": "UGA Campus Explorer Team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yourusername/uga-campus-explorer/issues"
  },
  "homepage": "https://github.com/yourusername/uga-campus-explorer#readme",
  "dependencies": {
    "@google/genai": "^1.40.0",
    "@tanstack/react-table": "^8.21.3"
  }
}
